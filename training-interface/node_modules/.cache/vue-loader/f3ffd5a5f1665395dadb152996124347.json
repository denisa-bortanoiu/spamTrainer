lt {\n  name: 'HelloWorld',\n  props: {\n    msg: String\n  },\n  data(){\n      return {\n        spam_files: [],\n        ham_files: []\n      }\n    },\n  methods: {\n    clearFiles(type) {\n      if (type === 'ham')\n        this.$refs.hamInput.reset();\n      else\n        this.$refs.spamInput.reset();\n    },\n    send_files(endpoint) {\n      let files = this.spam_files;\n      if (endpoint === \"ham\") {\n       files = this.ham_files;\n      }\n\n      console.log(endpoint);\n\n      let formData = new FormData();\n      for( var i = 0; i < this.files.length; i++ ){\n        let file = this.files[i];\n\n        formData.append('files[' + i + ']', file);\n      }\n\n      axios.post('http://localhost:5000/' + endpoint,\n        formData,\n        {\n          headers: {\n              'Content-Type': 'multipart/form-data'\n          }\n        }\n      ).then(function(){\n        console.log('SUCCESS!!');\n      })\n      .catch(function(){\n        console.log('FAILURE!!');\n      });\n    }\n  }\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n.buttons {\n  margin: 5px;\n}\n  h1 {\n    margin-bottom: 40px;\n  }\n</style>\n"]}]}